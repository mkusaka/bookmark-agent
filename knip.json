{
  "$schema": "https://unpkg.com/knip@5/schema.json",
  "entry": [
    "src/app/sw.ts"
  ],
  "project": [
    "src/**/*.{js,jsx,ts,tsx}"
  ],
  "ignore": [
    ".next/**",
    "node_modules/**",
    "public/**",
    "src/**/*.test.{ts,tsx}",
    "src/**/*.spec.{ts,tsx}",
    "src/test/**",
    "src/__tests__/**"
  ],
  "ignoreDependencies": [
    "tailwindcss",
    "eslint",
    "eslint-config-next"
  ],
  "ignoreExportsUsedInFile": {
    "type": true,
    "interface": true
  },
  "paths": {
    "@/*": ["./src/*"]
  }
}